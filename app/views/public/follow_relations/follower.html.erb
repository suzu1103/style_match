  <div class="row mt-5">
    <div class="col">
      <table class="table mt-5 ">
        <thead>
          <tr>
            <th scope="col">＃</th>
            <th scope="col">ユーザー名</th>
          </tr>
        </thead>

        <tbody>
        <% if @customers.empty? %>
          <th>フォローしているユーザーはいません</th>
        <% else %>
            <tr>
            <% @customers.each do |customer| %>
            <!--% @customers.where.not(id: current_customer.id).each do |customer| %>-->
              <td scope="row">
                  <%= image_tag customer.get_profile_image(50,50), class:"mt-3 rounded-circle"; %>
              </td>
              <td><%= customer.user_name %></td>
            <% end %>
            </tr>
        <% end %>
        </tbody>
      </table>
    </div>
  </div>